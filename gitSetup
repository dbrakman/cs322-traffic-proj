read -r -p "Enter your name: " NAME
git config --global user.name "$NAME"
read -r -p "Enter the email associated with your GitHub account: " EMAIL
git config --global user.email "$EMAIL"
git config --global credential.helper cache
printf "Clearing your Desktop (placing contents in ~/tmpDesktop)\n"
mkdir ~/tmpDesktop
for x in ~/Desktop/* ~/Desktop/.[!.]* ~/Desktop/..?*; do
  if [ -e "$x" ]; then mv -- "$x" ~/tmpDesktop/; fi
done
cd ~
printf "  $ git clone https://github.com/dbrakman/cs322-traffic-proj/ \n"
git clone https://github.com/dbrakman/cs322-traffic-proj/ Desktop
printf "  $ git checkout -b devel \n"
cd Desktop
git checkout -b devel
printf " Use \n"
printf "  $ git pull origin devel \n"
printf " to get the latest files from the Web, \n"
printf " and use the following sequence of commands to put your files online: \n"
printf "  $ git add file1 file2 ... \n" 
printf "  $ git commit -m 'my commit message' \n"
printf "  $ git push origin devel "
printf " \n"
printf " It's important that you add, commit, and push *in that order* \n"
